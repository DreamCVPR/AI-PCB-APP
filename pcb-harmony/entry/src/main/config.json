{
  "app": {
    "vendor": "example",
    "bundleName": "zjnu.huawei.pcb",
    "version": {
      "code": 1000000,
      "name": "1.0.0"
    }
  },
  "deviceConfig": {
    "default": {
      "allowComponentsProxy": true,
      "network": {
        "cleartextTraffic": true
      }
    }
  },
  "module": {
    "mainAbility": ".MainAbility",
    "deviceType": [
      "phone",
      "wearable",
      "tablet"
    ],
    "abilities": [
      {
        "skills": [
          {
            "entities": [
              "entity.system.home"
            ],
            "actions": [
              "action.system.home"
            ]
          }
        ],
        "orientation": "unspecified",
        "visible": true,
        "srcPath": "MainAbility",
        "name": ".MainAbility",
        "srcLanguage": "js",
        "icon": "$media:icon",
        "description": "$string:MainAbility_desc",
        "formsEnabled": false,
        "label": "$string:MainAbility_label",
        "type": "page",
        "launchType": "standard"
      }
    ],
    "distro": {
      "moduleType": "entry",
      "installationFree": false,
      "deliveryWithInstall": true,
      "moduleName": "entry"
    },
    "package": "zjnu.huawei.pcb",
    "name": ".entry",
    "js": [
      {
        "pages": [
          "pages/login/login"
//          "pages/index/index"
        ],
        "name": ".MainAbility",
        "window": {
          "designWidth": 720,
          "autoDesignWidth": false
        }
      }
    ],
    "reqPermissions":[
      {
        "name" : "ohos.permission.INTERNET"
      }
    ]
  }
}